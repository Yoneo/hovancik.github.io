---

title: Jan Hovancik, software developer - guitar player - poetry lover
priority: 1.0

---

<% content_for :header do %>
  <% content_tag :h1, :class => "index_name" do %>
    Jan Hovancik
  <% end %>
  <%= link_to image_tag("hovancik.jpg", :class => "photo index_photo"), "/blog", :title => "Visit Blog" %>
  <% content_tag :h2, :class => "index_desc" do %>
    software developer - guitar player - poetry lover
  <% end %>  
  <% content_tag :p do %>
    <%= link_to '<i class="fa fa-pencil-square fa-lg"></i>', "/blog", :title => "Visit Blog" %>
    <%= link_to '<i class="fa fa-linkedin-square fa-lg"></i>', "https://www.linkedin.com/in/janhovancik", :title => "LinkedIn profile" %>
    <%= link_to '<i class="fa fa-github-square fa-lg"></i>', "https://www.github.com/hovancik", :title => "Github" %>
    <%= link_to '<i class="fa fa-envelope-square fa-lg"></i>', "mailto:conta.srdr@gmail.com", :title => "Email" %>
    <%= link_to '<i class="fa fa-twitter-square fa-lg"></i>', "https://www.twitter.com/JanHovancik", :title => "Twitter" %>
  <% end %>
<% end %>
<body>
  <div id="latest_post">Latest post in blog: <%= link_to blog.articles[0].title, blog.articles[0].url  %> <i class="fa fa-times-circle" onclick="document.getElementById('latest_post').style.visibility='hidden'"></i></div>
</body>


